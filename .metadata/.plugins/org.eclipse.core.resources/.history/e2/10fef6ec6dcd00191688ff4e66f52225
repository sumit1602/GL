package com.example.demo;

import org.springframework.stereotype.Component;

@Component
public class Alien {
	
	private String name;
	private int aid;
	

	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public int getAid() {
		return aid;
	}


	public void setAid(int aid) {
		this.aid = aid;
	}
	
	public void show() {
		System.out.print("in show");
	}

}
